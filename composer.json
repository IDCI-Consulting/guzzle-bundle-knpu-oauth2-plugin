{
    "name": "idci/guzzle-bundle-oauth2-plugin",
    "type": "symfony-bundle",
    "description": "This bundle integrates OAuth2 functionality using a Guzzle Bundle plugin.",
    "license": "MIT",
    "keywords": ["Guzzle", "OAuth2"],
    "authors": [
        {
            "name": "Gabriel BONDAZ",
            "email": "gabriel.bondaz@idci-consulting.fr"
        },
        {
            "name": "Camille SCHWARZ",
            "email": "camille54460@gmail.com"
        }
    ],
    "require": {
        "symfony/dependency-injection": "^5.3|^6.0",
        "eightpoints/guzzle-bundle": "^7.0|^8.0",
        "knpuniversity/oauth2-client-bundle": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "symfony/stopwatch": "^5.3|^6.0",
        "symfony/twig-bundle": "^5.3|^6.0"
    },
    "autoload": {
        "psr-4": {
            "IDCI\\Bundle\\GuzzleBundleOAuth2PluginBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "IDCI\\Bundle\\GuzzleBundleOAuth2PluginBundle\\Tests\\": "Tests"
        }
    }
}
